{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard-with-typescript", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "max-len": ["error", { "code": 80, "ignoreUrls": true }],
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
